{
  "name": "@adobe/helix-cli",
  "version": "4.9.9",
  "description": "Project Helix CLI",
  "main": "index.js",
  "bin": {
    "hlx": "./index.js"
  },
  "scripts": {
    "check": "npm run lint && npm run test",
    "test": "nyc --lines 90 --reporter=text --reporter=lcov mocha -t 5000",
    "test-ci": "circleci tests glob 'test/test*.js' | circleci tests split --split-by=timings | xargs nyc --reporter=text --reporter=lcov mocha -R mocha-junit-reporter -t 5000 && codecov",
    "test-ci-win": "mocha -R mocha-junit-reporter -t 5000",
    "lint": "./node_modules/.bin/eslint .",
    "lint-ci": "circleci tests glob 'src/**/*.js' 'test/**/*.js' | circleci tests split | xargs eslint",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-cli"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/project-helix/issues"
  },
  "homepage": "https://github.com/adobe/project-helix#readme",
  "peerDependencies": {
    "isomorphic-git": "0.x"
  },
  "engines": {
    "node": ">=8.9 <9.0 || >=10.0 < 11.0"
  },
  "dependencies": {
    "@adobe/fastly-native-promises": "^1.10.0",
    "@adobe/helix-pipeline": "4.0.5",
    "@adobe/helix-shared": "2.1.1",
    "@adobe/helix-simulator": "2.12.18",
    "@adobe/htlengine": "3.2.1",
    "@babel/core": "^7.5.5",
    "@parcel/logger": "^1.11.0",
    "@snyk/nodejs-runtime-agent": "^1.46.0",
    "archiver": "^3.0.3",
    "camelcase": "^5.3.1",
    "chalk": "2.4.2",
    "chokidar": "^3.0.2",
    "dotenv": "^8.0.0",
    "ferrum": "^1.1.1",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.4",
    "glob-to-regexp": "0.4.1",
    "hyperscript": "^2.0.2",
    "ignore": "^5.1.2",
    "ini": "^1.3.5",
    "junit-report-builder": "1.3.1",
    "lodash": "^4.17.15",
    "mime-types": "^2.1.24",
    "object-hash": "^1.3.1",
    "open": "^6.4.0",
    "openwhisk": "^3.19.0",
    "parcel-bundler": "^1.12.3",
    "postcss": "^7.0.17",
    "postcss-url": "^8.0.0",
    "postcss-value-parser": "^4.0.0",
    "progress": "^2.0.3",
    "request": "^2.88.0",
    "request-promise-native": "1.0.7",
    "sanitizer": "^0.1.3",
    "semver": "^6.2.0",
    "shelljs": "^0.8.3",
    "snyk": "^1.195.1",
    "source-map": "^0.7.3",
    "uri-js": "^4.2.2",
    "uuid": "^3.3.2",
    "webpack": "^4.36.0",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@adobe/helix-testutils": "^0.2.0",
    "@pollyjs/adapter-fetch": "^2.6.0",
    "@pollyjs/adapter-node-http": "^2.6.0",
    "@pollyjs/core": "^2.6.0",
    "@pollyjs/persister-fs": "^2.6.0",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "@semantic-release/github": "^5.4.2",
    "codecov": "^3.5.0",
    "commitizen": "^4.0.1",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "js-yaml": "^3.13.1",
    "jsdom": "^15.1.1",
    "lint-staged": "^9.2.0",
    "mocha": "^6.2.0",
    "mocha-junit-reporter": "^1.23.0",
    "mocha-parallel-tests": "^2.2.1",
    "nock": "^10.0.6",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.0",
    "semantic-release": "^15.13.19",
    "sinon": "^7.3.2",
    "unzip2": "^0.2.5"
  },
  "snyk": true,
  "lint-staged": {
    "*.js": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npx lint-staged"
    }
  }
}
