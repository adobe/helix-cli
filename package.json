{
  "name": "@adobe/helix-cli",
  "version": "6.1.0",
  "description": "Project Helix CLI",
  "main": "index.js",
  "bin": {
    "hlx": "./index.js"
  },
  "scripts": {
    "check": "npm run lint && npm run test",
    "test": "nyc --lines 90 --reporter=text --reporter=lcov mocha -t 5000",
    "test-ci": "circleci tests glob 'test/test*.js' | circleci tests split --split-by=timings | xargs nyc --reporter=text --reporter=lcov mocha -R mocha-junit-reporter -t 5000 && codecov",
    "test-ci-win": "./node_modules/.bin/mocha --reporter xunit test --reporter-options output=junit/test.xml -t 5000",
    "lint": "./node_modules/.bin/eslint .",
    "lint-ci": "circleci tests glob 'src/**/*.js' 'test/**/*.js' | circleci tests split | xargs eslint",
    "semantic-release": "semantic-release",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-cli"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/project-helix/issues"
  },
  "homepage": "https://github.com/adobe/project-helix#readme",
  "engines": {
    "node": ">=8.9 <9.0 || >=10.0 <11.0 || >=12.11.1 <13.0"
  },
  "dependencies": {
    "@adobe/fastly-native-promises": "^1.12.0",
    "@adobe/helix-shared": "3.1.2",
    "@adobe/helix-simulator": "2.17.5",
    "@adobe/htlengine": "3.2.8",
    "@babel/core": "^7.7.0",
    "@babel/plugin-transform-react-jsx": "^7.7.0",
    "archiver": "^3.1.1",
    "camelcase": "^5.3.1",
    "chalk": "3.0.0",
    "chokidar": "^3.0.2",
    "dotenv": "^8.0.0",
    "ferrum": "^1.1.1",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.4",
    "glob-to-regexp": "0.4.1",
    "hyperscript": "^2.0.2",
    "ignore": "^5.1.4",
    "ini": "^1.3.5",
    "isomorphic-git": "0.70.7",
    "junit-report-builder": "1.3.3",
    "lodash": "^4.17.15",
    "open": "^7.0.0",
    "openwhisk": "^3.19.0",
    "progress": "^2.0.3",
    "request": "^2.88.0",
    "request-promise-native": "1.0.8",
    "semver": "^6.3.0",
    "shelljs": "^0.8.3",
    "uuid": "^3.3.2",
    "webpack": "^4.39.2",
    "yargs": "15.0.2"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "1.1.0",
    "@adobe/helix-testutils": "0.2.0",
    "@pollyjs/adapter-fetch": "2.6.3",
    "@pollyjs/adapter-node-http": "2.7.0",
    "@pollyjs/core": "2.6.3",
    "@pollyjs/persister-fs": "2.6.3",
    "@semantic-release/changelog": "3.0.6",
    "@semantic-release/git": "7.0.18",
    "@semantic-release/github": "5.5.5",
    "codecov": "3.6.1",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.7.0",
    "eslint-plugin-header": "3.0.0",
    "eslint-plugin-import": "2.18.2",
    "jsdom": "15.2.1",
    "lint-staged": "9.4.3",
    "mocha": "6.2.2",
    "mocha-junit-reporter": "1.23.1",
    "nock": "11.7.0",
    "nyc": "14.1.1",
    "proxyquire": "2.1.3",
    "semantic-release": "15.13.31",
    "sinon": "7.5.0",
    "yauzl": "2.10.0"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npx lint-staged"
    }
  }
}
